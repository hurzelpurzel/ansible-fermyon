[Unit]
Description=nomad
Requires=network.target
After=syslog.target network.target

[Service]
Type=forking
ExecStart={{nomad_home}}/start.sh
User=nomad
EnvironmentFile={{ nomad_home }}/nomad.env
PIDFile={{nomad_home}}/nomad.pid
Environment="CONUL_PID_FILE_PATH={{nomad_home}}/nomad.pid"
TimeoutSec=300

[Install]
WantedBy=multi-user.target
